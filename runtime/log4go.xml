<logging>
    <filter enabled="true">
        <tag>file</tag>
        <type>file</type>
        <level>DEBUG</level>
        <property name="filename">./output/logs/barrage.log</property>
        <property name="format">[%D %T] [%L] %M</property>
        <property name="rotate">true</property> 
        <property name="daily">true</property> 
    </filter>
</logging>
